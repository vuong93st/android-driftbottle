<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_height="50dip" 
   xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="wrap_content"
   android:id="@+id/relativeLayout1">
    <ImageView android:id="@+id/yqljItemLogo" android:layout_width="wrap_content" 
	android:layout_height="wrap_content" android:layout_alignParentLeft="true" 
	android:layout_centerVertical="true"/>
    <TextView android:id="@+id/yqljItemTitle" android:layout_alignParentRight="true"
		android:textSize="16sp" android:textColor="#000000" android:layout_centerHorizontal="true"
		android:layout_width="wrap_content" android:layout_height="wrap_content" 
		android:layout_centerVertical="true"/>
</RelativeLayout>
